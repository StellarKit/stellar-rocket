{
  "name": "stellar-rocket",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "github.com/StellarKit/stellar-rocket",
  "description": "Floating stock ticker and simple Stellar wallet",
  "homepage": "https://stellarkit.io",
  "author": {
    "name": "Steve Gehrman",
    "email": "steve@distantfutu.re"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "electron-webpack && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "asar": true,
    "productName": "Stellar Rocket",
    "appId": "io.stellarkit.stellar-rocket",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "createDesktopShortcut": false
    }
  },
  "dependencies": {
    "electron-context-menu": "^0.9.1",
    "jquery": "^3.3.1",
    "md5": "^2.2.1",
    "nconf": "^0.10.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "sillyname": "^0.1.0",
    "source-map-support": "^0.5.4",
    "stellar-sdk": "^0.8.0",
    "stellar-js-utils": "git://github.com/StellarKit/stellarkit-js-utils",
    "gsap": "^1.20.4"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "1.8.4",
    "electron-builder": "^20.8.1",
    "electron-webpack": "2.0.1",
    "electron-webpack-eslint": "^2.1.0",
    "electron-webpack-vue": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "4.4.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuetify": "^1.0.13",
    "webpack": "^4.5.0",
    "webpack-build-notifier": "^0.1.23"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
